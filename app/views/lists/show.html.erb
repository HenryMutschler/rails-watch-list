<div class="list-show-card">
        <h1><%= @list.name %></h1>
    </div>

    <ul class='list-group'>
  <% @list.bookmarks.each do |bookmark| %>
    <li class="list-group-item">
      <%= bookmark.movie[:title] %>
      <%= bookmark.comment %>
    </li>
  <% end %>
</ul>
    <%= link_to 'Add a bookmark', new_bookmark_path(@list) %><br>
    <%= link_to 'Back', lists_path %>